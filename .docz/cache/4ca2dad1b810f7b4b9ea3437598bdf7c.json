iB;AAAA,CAAnB;;AASAgC,UAAU,CAACE,YAAX,GAA0B;AACxBjC,EAAAA,iBAAiB,EAAE;AAAA,WAAML,IAAI,CAAC;AAACuC,MAAAA,eAAe,EAAE;AAAlB,KAAD,CAAV;AAAA,GADK;AAExBjC,EAAAA,YAAY,EAAE,oBAAC,QAAD;AAAU,IAAA,EAAE,EAAC,GAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAFU;AAGxBC,EAAAA,aAAa,EAAE,8BAHS;AAIxBE,EAAAA,UAAU,EAAE,MAJY;AAKxBD,EAAAA,aAAa,EAAE;AALS,CAA1B;AAQA4B,UAAU,CAACI,SAAX,GAAuB;AACrBnC,EAAAA,iBAAiB,EAAEN,SAAS,CAAC0C,SAAV,CAAoB,CACrC1C,SAAS,CAACiB,MAD2B,EAErCjB,SAAS,CAAC2C,MAF2B,EAGrC3C,SAAS,CAAC4C,IAH2B,CAApB,CADE;AAMrBrC,EAAAA,YAAY,EAAEP,SAAS,CAAC6C,OANH;AAOrBrC,EAAAA,aAAa,EAAER,SAAS,CAACiB,MAPJ;AAQrBP,EAAAA,UAAU,EAAEV,SAAS,CAACiB,MARD;AASrBR,EAAAA,aAAa,EAAET,SAAS,CAACiB;AATJ,CAAvB;eAYeoB,U;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAtFanC,iB;0BAGPE,kB;0BAsDAiC,U","sourcesContent":["import React, { useState, useEffect, useMemo } from 'react';\nimport { withRouter, matchPath, Redirect } from 'react-router-dom';\nimport PropTypes from 'prop-types';\n\nimport { fade } from './tigers';\n\nexport const NavigationContext = React.createContext();\n\n\nconst NavigationProvider = withRouter(({\n  children,\n  defaultTransition,\n  defaultRoute,\n  bodyClassName,\n  rootClassName,\n  rootNodeId,\n\n  match,\n  location,\n  history,\n}) => {\n\n  const [transition, setTransition] = useState(defaultTransition)\n\n  const context = {\n    transition: transition,\n    setTransition: string => new Promise(function(resolve, reject) {\n        resolve(setTransition(string))\n    }),\n    defaultTransition: defaultTransition\n  }\n\n  useEffect(() => {\n    document.body.classList.add(bodyClassName);\n    document.getElementById(rootNodeId).classList.add(rootClassName);\n\n    return () => {\n      document.body.classList.remove(bodyClassName);\n      document.getElementById(rootNodeId).classList.remove(rootClassName);\n    };\n  }, []);\n\n  const matched = useMemo(() => (\n    children.some((child, i) => {\n      const matchedPath = matchPath(location.pathname, {\n        path: child.props.path,\n        exact: child.props.exact || false,\n        strict: child.props.strict || false,\n      });\n      if (matchedPath != null){\n        return true\n      }\n    })\n  ), [location.pathname, children])\n\n  return (\n    <NavigationContext.Provider value={context}>\n      {children}\n      {!matched && defaultRoute}\n    </NavigationContext.Provider>\n  )\n})\n\nconst Navigation = ({\n  children,\n  ...other,\n}) => (\n  <NavigationProvider {...other}>\n    {children}\n  </NavigationProvider>\n)\n\nNavigation.defaultProps = {\n  defaultTransition: () => fade({backgroundColor: 'white'}),\n  defaultRoute: <Redirect to='/' />,\n  bodyClassName: 'react-tiger-transition--body',\n  rootNodeId: 'root',\n  rootClassName: 'react-tiger-transition--root',\n};\n\nNavigation.propTypes = {\n  defaultTransition: PropTypes.oneOfType([\n    PropTypes.string,\n    PropTypes.object,\n    PropTypes.func,\n  ]),\n  defaultRoute: PropTypes.element,\n  bodyClassName: PropTypes.string,\n  rootNodeId: PropTypes.string,\n  rootClassName: PropTypes.string\n}\n\nexport default Navigation\n"]}]}